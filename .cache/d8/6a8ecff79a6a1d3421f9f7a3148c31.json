   * Next action\n     */\n    next({ type: REQUEST });\n\n    const { payload = {}, headers = {} } = promise;\n\n    return axios({\n      method: \"POST\",\n      url: confgi.api.url,\n      data: payload,\n      headers: headers\n    })\n      .then(({ result: response }) => {\n        next({ response, type: SUCCESS });\n        return true;\n      })\n      .catch(error => {\n        next({ payload: { type: FAILURE }, type: MYRESPONSE });\n        return false;\n      });\n  }\n};\n\nexport default plugin;\n"},"lineCount":null}},"hash":"ebb8529378f0f506380669586f81e38e","cacheData":{"env":{}}}