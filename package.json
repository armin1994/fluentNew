{
  "name": "valegoo-revamp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "start:admin": "npm run dev:admin",
    "start:recorder": "npm run dev:recorder",
    "start:cleaning": "npm run dev:cleaning",
    "buildAll": "npm run prod:build && npm run prod:build-admin && npm run prod:build-recorder && npm run prod:build-cleaning",
    "dev:admin": "npm-run-all --parallel dev:build-admin dev:serve",
    "dev:recorder": "npm-run-all --parallel dev:build-recorder dev:serve",
    "dev:cleaning": "npm-run-all --parallel dev:build-cleaning dev:serve",
    "dev:build": "npm run clean && cross-env NODE_ENV=development BABEL_ENV=client parcel watch ./App.js --out-dir dist/bundles --public-url /dist",
    "dev:build-admin": "npm run clean && cross-env NODE_ENV=development BABEL_ENV=client parcel watch ./admin.js --out-dir dist/bundles --public-url /dist",
    "dev:build-recorder": "npm run clean && cross-env NODE_ENV=development BABEL_ENV=client parcel watch ./Recorder.js --out-dir dist/bundles --public-url /dist",
    "dev:build-cleaning": "npm run clean && cross-env NODE_ENV=development BABEL_ENV=client parcel watch ./CleaningCS.js -fpa-out-dir dist/bundles --public-url /dist",
    "dev:serve": "cross-env NODE_ENV=development BABEL_ENV=server npx nodemon",
    "prod:build-admin": "cross-env NODE_ENV=production BABEL_ENV=client BUILD_TYPE=Admin node ./parcel.config.js",
    "prod:build-recorder": "cross-env NODE_ENV=production BABEL_ENV=client BUILD_TYPE=Recorder node ./parcel.config.js",
    "prod:build-cleaning": "cross-env NODE_ENV=production BABEL_ENV=client BUILD_TYPE=CleaningCS node ./parcel.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://nileshsavani@bitbucket.org/pappu8300/valegoo-revamp.git"
  },
